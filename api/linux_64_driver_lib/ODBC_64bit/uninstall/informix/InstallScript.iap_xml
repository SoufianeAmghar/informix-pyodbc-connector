											<property name="script">
												<string><![CDATA[@ECHO OFF
SETLOCAL

SET "sourcedir=$INST_RES_DIR$"
SET "serviceName=$DRIVER_SHORT_NAME$.service.name"
SET "exeName=$DRIVER_SHORT_NAME$.service.exe.name"
SET "servicePort=$DRIVER_SHORT_NAME$.service.jvmport"
SET "valueServiceName=$SERVICE_NAME$"
SET "valueExeName=$SERVICE_EXE_NAME$"
SET "valueServicePort=$SERVICE_JVMPORT$"
SET "existingDriver=0"
(
 FOR /f "usebackqdelims=" %%a IN ("$SERVICE_PROPS_FILE$") DO (
  FOR /f "tokens=1*delims==" %%g IN ("%%a") DO (

  IF /i "%%g"=="%serviceName%" (
	ECHO %%g=%valueServiceName%
	SET "existingDriver=1"
	) ELSE IF /i "%%g"=="%exeName%" (
		ECHO %%g=%valueExeName%
		) ELSE IF /i "%%g"=="%servicePort%" (
  		ECHO %%g=%valueServicePort%
  		) ELSE (
		ECHO(%%a)
		)
  )
  
)>newfile.txt
:: newfile.txt now contains a modified version.

:: This line will append the new file if the driver does not already exist
  IF "%existingDriver%" == "0" (
	REM ECHO "Driver exists=%existingDriver%"
	ECHO %serviceName%=%valueServiceName%
	ECHO %exeName%=%valueExeName%
	ECHO %servicePort%=%valueServicePort%
  )>>newfile.txt

:: This line will overwrite the original
ECHO MOVE /y newfile.txt "$SERVICE_PROPS_FILE$"
MOVE /y newfile.txt "$SERVICE_PROPS_FILE$"


GOTO :EOF
]]></string>
